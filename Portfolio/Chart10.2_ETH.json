{"$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
      "text": "ETH/USD",
      "fontSize":17,
      "anchor": "start"
    },
    "data": {
      "url": "https://raw.githubusercontent.com/AidanTrue/AidanTrue.github.io/main/CryptoLoopedData-ETH.json",
      "format": {"type": "json"}
    },

    "height": 300,
    "width": 300,
  
    "background": null,
    
    "mark": {
        "type": "line",
        "point": false,
        "color": "darkviolet"},

    "encoding": {
      "x": {
        "field": "time_period_end",
        "type": "temporal",
        "title": "Date",
        "axis": {"grid": false}
        
      },
      "y": {
        "field": "price_close",
        "title": "Price (USD)",
        "type": "quantitative"
      }
    }
}